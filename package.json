{
  "name": "twitter-wall-ui",
  "version": "1.0.0",
  "description": "Display tweets with webgl",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js --open --live --verbose --debug",
    "build": "browserify index.js -t es2040 | indexhtmlify > index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaoodAlvi/twitter-wall-ui.git"
  },
  "keywords": [
    "twitter",
    "webgl",
    "animation"
  ],
  "author": "Saood Alvi",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SaoodAlvi/twitter-wall-ui/issues"
  },
  "homepage": "https://github.com/SaoodAlvi/twitter-wall-ui#readme",
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "dependencies": {
    "2d-context": "^1.3.0",
    "gl-mat3": "^1.0.0",
    "gl-mat4": "^1.1.4",
    "gl-vec3": "^1.0.3",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-noise": "0.0.0",
    "glslify": "^6.1.0",
    "orbit-controls": "^1.2.4",
    "perspective-camera": "^2.0.1",
    "regl": "^1.3.1"
  },
  "devDependencies": {
    "browserify": "^15.2.0",
    "budo": "^10.0.4",
    "es2040": "^1.2.6",
    "indexhtmlify": "^1.3.1"
  }
}
